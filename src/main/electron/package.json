{
  "name": "designer-for-flow",
  "version": "14.4.0-alpha.2",
  "main": "./main.js",
  "license": "GPL-3.0",
  "description": "Electron wrapper for 'Designer for Flow'",
  "dependencies": {
    "minimal-request-promise": "^1.5.0",
    "get-port": "^5.1.1",
    "decompress": "^4.2.1"
  },
  "scripts": {
    "package": "electron-packager ./ --icon=icon.ico --ignore=node\\b",
    "package-windows": "electron-packager ./ --icon=icon.ico  --platform=win32",
    "package-linux": "electron-packager ./ --icon=icon.ico  --platform=linux",
    "package-mac-x64": "electron-packager ./ --icon=icon.ico --platform=darwin",
    "package-mac-arm64": "electron-packager ./ --icon=icon.ico --platform=darwin --arch=arm64",
    "package-all": "electron-packager ./ --icon=icon.ico --platform=all"
  },
  "devDependencies": {
    "electron-packager": "^15.2.0",
    "electron": "^12.0.0"
  }
}
